[__PRIVREPO__-sclo-sclo]
name=CentOS-7 - SCLo sclo (__PRIVREPO__)
baseurl=http://__MIRROR_HOST__/__PATH_ROOT__/7/sclo/$basearch/sclo/
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-SCLo

