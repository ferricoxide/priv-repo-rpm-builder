[__PRIVREPO__-epel]
name=Extra Packages for Enterprise Linux 7 - $basearch (__PRIVREPO__)
baseurl=http://__MIRROR_HOST__/__PATH_ROOT__/epel/7/$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-7

[__PRIVREPO__-epel-debuginfo]
name=Extra Packages for Enterprise Linux 7 - $basearch - Debug (__PRIVREPO__)
baseurl=http://__MIRROR_HOST__/__PATH_ROOT__/epel/7/$basearch/debug
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-7
gpgcheck=1

[__PRIVREPO__-epel-source]
name=Extra Packages for Enterprise Linux 7 - $basearch - Source (__PRIVREPO__)
baseurl=http://__MIRROR_HOST__/__PATH_ROOT__/epel/7/SRPMS
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-7
gpgcheck=1

